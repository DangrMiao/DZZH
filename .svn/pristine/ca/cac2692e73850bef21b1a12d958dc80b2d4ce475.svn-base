<!DOCTYPE html>
<meta charset="UTF-8">
<link rel="stylesheet" href="../../ace/css/bootstrap.css">
<link rel="stylesheet" href="../../ace/css/bootstrap-table.min.css">
<script type="text/javascript" src="../../ace/js/jquery.js"></script>
<script src="../../ace/js/bootstrap.js"></script>
<!-- 弹出提示消息 -->
<script src="../../html_UI/assets/js/jquery.gritter.js"></script>
<link rel="stylesheet" href="../../html_UI/assets/css/jquery.gritter.css" />
<script src="../../bootstrap-growl/jquery.bootstrap-growl.js"></script>
<!-- 表单 -->
<link rel="stylesheet" href="../../css/form.css">
<!-- 下拉框 -->
<link rel="stylesheet" href="../../ace/css/bootstrap-multiselect.css" />
<script src="../../ace/js/bootstrap-multiselect.js"></script>
<!-- 表格 -->	
<script src="../../ace/js/dataTables/bootstrap-table.js"></script>
<script src="../../ace/js/dataTables/bootstrap-table-zh-CN.js"></script>
<!-- 删除时确认窗口 -->
<script src="../../ace/js/bootbox.js"></script>
	
<script src="../../js/common/commonUtils.js" type="text/javascript"></script>
<script type="text/javascript" src="../../js/ReportManager/Config.js"></script>
<script type="text/javascript" src="../../js/systemManager/areaManager.js"></script>
<body>
<div id="area-Manager-data-div" style="position: absolute;bottom: 0px;right: 0px;top: 0px;left: 0px;">
	<table id="area-Manager-data" style="background-color: rgba(242, 242, 242, 0.9);" class="table table-striped table-bordered table-hover no-border-top">			
	</table>
</div>
	
<!-- 辖区编辑的模态框 -->
<div class="modal fade" id="area-Manager-dialog" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content"  style="width:550px;">
			<div class="modal-header" style="padding: 10px;">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
				<h5 class="modal-title" id="">辖区编辑</h5>
			</div>
			<div class="modal-body" style="height: 350px;">
				<form id="area-Manager-dialog-form" class="form-horizontal" role="form">					
					<div id="area-Manager-dialog-data-div" style="position: absolute;bottom: 0px;right: 0px;top: 0px;left: 0px;">
						<table id="area-Manager-dialog-data" style="background-color: rgba(242, 242, 242, 0.9);" class="table table-striped table-bordered table-hover no-border-top">			
						</table>
					</div>
					
				</form>
			</div>
			<div class="modal-footer" style="text-align: center;">
				<button id="area-Manager-add-dialog-save" type="button" class="btn btn-primary ">确认</button>
				<button type="button" class="btn btn-default " data-dismiss="modal">关闭</button>
			</div>
		</div><!-- /.modal-content -->
	</div><!-- /.modal -->
</div>


<script type="text/javascript">
$(top.hangge());
</script>
</body>